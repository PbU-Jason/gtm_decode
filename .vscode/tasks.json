{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "debug build",
			"command": "/usr/bin/gcc",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"*.c",
				"-o",
				"${fileDirname}/gtm_decoder"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: /usr/bin/gcc"
		}
	]
}